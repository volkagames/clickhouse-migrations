{
  "name": "@volkagames/clickhouse-migrations",
  "version": "2.6.0",
  "description": "ClickHouse Migrations",
  "bin": {
    "clickhouse-migrations": "dist/cli.js"
  },
  "main": "dist/migrate.cjs",
  "module": "dist/migrate.js",
  "types": "dist/migrate.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/migrate.d.ts",
      "import": "./dist/migrate.js",
      "require": "./dist/migrate.cjs"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.0.0",
  "private": false,
  "license": "MIT",
  "author": "alfatm",
  "contributors": [
    {
      "name": "VVVi",
      "url": "https://github.com/VVVi"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/volkagames/clickhouse-migrations.git"
  },
  "bugs": {
    "url": "https://github.com/volkagames/clickhouse-migrations/issues"
  },
  "homepage": "https://github.com/volkagames/clickhouse-migrations#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:unit": "vitest run tests/*.unit.test.ts",
    "test:integration": "vitest run tests/*.integration.test.ts",
    "test:e2e": "pnpm run test:e2e:setup && pnpm run test:e2e:wait && vitest run tests/*.e2e.test.ts",
    "test:e2e:setup": "docker compose up -d clickhouse clickhouse_tls",
    "test:e2e:wait": "bash -c 'for i in {1..30}; do if curl -s http://localhost:8123/ping > /dev/null 2>&1 && echo \"SELECT 1\" | curl -s http://localhost:8123/ --data-binary @- > /dev/null 2>&1; then echo \"ClickHouse ready\"; exit 0; fi; sleep 1; done; echo \"Timeout\"; exit 1'",
    "test:e2e:teardown": "docker compose down",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "format": "biome format --write ./src ./tests",
    "lint": "biome lint ./src ./tests",
    "check": "biome check --write ./src ./tests",
    "prepare": "husky && pnpm run build",
    "prepublishOnly": "pnpm run test:unit && pnpm run check",
    "preversion": "pnpm run check",
    "version": "pnpm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@clickhouse/client": "^1.14.0",
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.2",
    "@vitest/coverage-v8": "4.0.15",
    "@vitest/ui": "^4.0.15",
    "husky": "^9.1.7",
    "rollup": "^4.53.3",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "clickhouse-migrations",
    "clickhouse-migration",
    "clickhouse-migrate",
    "clickhouse",
    "migrations",
    "migration",
    "migrate",
    "cli"
  ]
}
