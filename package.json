{
  "name": "@volkagames/clickhouse-migrations",
  "version": "2.0.1",
  "description": "ClickHouse Migrations",
  "bin": {
    "clickhouse-migrations": "dist/cli.js"
  },
  "main": "dist/migrate.cjs",
  "module": "dist/migrate.js",
  "types": "dist/migrate.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/migrate.d.ts",
      "import": "./dist/migrate.js",
      "require": "./dist/migrate.cjs"
    },
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20",
    "bun": ">=1.2.23",
    "npm": "please-use-bun",
    "yarn": "please-use-bun",
    "pnpm": "please-use-bun"
  },
  "packageManager": "bun@1.2.23",
  "private": false,
  "license": "MIT",
  "author": "alfatm",
  "contributors": [
    {
      "name": "VVVi",
      "url": "https://github.com/VVVi"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/volkagames/clickhouse-migrations.git"
  },
  "bugs": {
    "url": "https://github.com/volkagames/clickhouse-migrations/issues"
  },
  "homepage": "https://github.com/volkagames/clickhouse-migrations#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "preinstall": "npx only-allow bun",
    "build": "rollup -c",
    "test": "vitest run",
    "test:unit": "vitest run tests/*.unit.test.ts",
    "test:integration": "vitest run tests/*.integration.test.ts",
    "test:e2e": "vitest run tests/*.e2e.test.ts",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "format": "biome format --write ./src ./tests",
    "lint": "biome lint ./src ./tests",
    "check": "biome check --write ./src ./tests",
    "prepare": "husky && bun run build",
    "prepublishOnly": "bun run test && bun run check",
    "preversion": "bun run check",
    "version": "bun run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@clickhouse/client": "^1.14.0",
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "4.0.13",
    "@vitest/ui": "^4.0.13",
    "husky": "^9.1.7",
    "rollup": "^4.53.3",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "clickhouse-migrations",
    "clickhouse-migration",
    "clickhouse-migrate",
    "clickhouse",
    "migrations",
    "migration",
    "migrate",
    "cli"
  ]
}
